<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
  <persistence-unit name="PartidaPersistenceUnit" transaction-type="JTA">
    <jta-data-source>jdbc/padel_db</jta-data-source>
    <class>com.partidas.padel.RWSPadel.domain.Partida</class>
    <properties>
      <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
   <persistence-unit name="JugadoresPersistenceUnit" transaction-type="JTA">
    <jta-data-source>jdbc/padel_db</jta-data-source>
    <class>com.partidas.padel.RWSPadel.domain.Jugadores</class>
    <properties>
      <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="UbicacionPersistenceUnit" transaction-type="JTA">
    <jta-data-source>jdbc/padel_db</jta-data-source>
    <class>com.partidas.padel.RWSPadel.domain.Ubicacion</class>
    <properties>
      <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
  <persistence-unit name="ParejaPersistenceUnit" transaction-type="JTA">
    <jta-data-source>jdbc/padel_db</jta-data-source>
    <class>com.partidas.padel.RWSPadel.domain.Pareja</class>
    <properties>
      <property name="eclipselink.logging.level" value="FINEST"/>
    </properties>
  </persistence-unit>
</persistence>
